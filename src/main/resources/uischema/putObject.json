                },
                {
                  "type": "attribute",
                  "value": {
                    "name": "objectLockMode",
                    "displayName": "Object Lock Mode",
                    "inputType": "comboOrExpression",
                    "comboValues": [
                      "GOVERNANCE",
                      "COMPLIANCE"
