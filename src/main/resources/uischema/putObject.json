                },
                {
                  "type": "attribute",
                  "value": {
                    "name": "grantRead",
                    "displayName": "Grant Read",
                    "inputType": "stringOrExpression",
                    "defaultValue": "",
                    "required": "false",
                    "helpTip": "The comma separated type(id, uri, emailAddress)=value pair to grant READ objects permission to those accounts."
                  }
                },
                {
                  "type": "attribute",
                  "value": {
                    "name": "grantReadACP",
                    "displayName": "Grant Read ACP",
