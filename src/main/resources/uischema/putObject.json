                  "type": "attribute",
                  "value": {
                    "name": "contentEncoding",
                    "displayName": "Content Encoding",
                    "inputType": "stringOrExpression",
                    "defaultValue": "",
                    "required": "false",
                    "helpTip": "This specifies what content encodings have been applied to the object."
