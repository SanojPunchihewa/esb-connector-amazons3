{
  "connectorName": "amazons3",
  "operationName": "putObject",
  "title": "Put Object",
  "help": "<h1>Put Object</h1> <b>The putObject operation adds an object to a bucket.</b><br><br><ul><li><a href=\"https://ei.docs.wso2.com/en/7.1.0/micro-integrator/references/connectors/amazons3-connector/amazons3-connector-reference/\"> More Help </a></li></ul>",
  "elements": [
    {
      "type": "attributeGroup",
      "value": {
        "groupName": "General",
        "elements": [
          {
            "type": "attribute",
            "value": {
              "name": "configRef",
              "displayName": "Amazon S3 Connection",
              "inputType": "connection",
              "allowedConnectionTypes": [
                "amazons3"
              ],
              "defaultType": "connection.amazons3",
              "defaultValue": "",
              "required": "true",
              "helpTip": "Connection to be used"
            }
          },
          {
            "type": "attributeGroup",
            "value": {
              "groupName": "Basic",
              "elements": [
                {
                  "type": "attribute",
                  "value": {
                    "name": "bucketName",
                    "displayName": "Bucket Name",
                    "inputType": "stringOrExpression",
                    "defaultValue": "",
                    "required": "true",
                    "helpTip": "The name of the bucket."
                  }
                },
                {
                  "type": "attribute",
                  "value": {
                    "name": "objectKey",
                    "displayName": "Object Key",
                    "inputType": "stringOrExpression",
                    "defaultValue": "",
                    "required": "true",
                    "helpTip": "The name to give the newly created object."
                  }
                },
                {
                  "type": "attribute",
                  "value": {
                    "name": "fileContent",
                    "displayName": "File Content",
                    "inputType": "stringOrExpression",
                    "defaultValue": "",
                    "required": "false",
                    "helpTip": "The content of the file."
                  }
                },
                {
                  "type": "attribute",
                  "value": {
                    "name": "filePath",
                    "displayName": "File Path",
                    "inputType": "stringOrExpression",
                    "defaultValue": "",
                    "required": "false",
                    "helpTip": "The path of the file to be uploaded."
                  }
                },
                {
                  "type": "attribute",
                  "value": {
                    "name": "acl",
                    "displayName": "Canned ACL",
